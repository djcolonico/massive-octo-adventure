<h1>Chatrooms</h1>

<%= button_to 'Log out', logout_path, :method => :delete, :controller => "sessions" %>

<ul>
  <% @chatrooms.each do |chatroom| %>
    <li>
      <%= link_to chatroom.name, chatroom_path(chatroom) %>
    </li>
  <% end %>
</ul>

<%= link_to "New Chatroom", new_chatroom_path, :class => "new_chatroom" %>
